<#@ template language="C#" #>
<#@ assembly name="System.Core" #>
<#@ import namespace="System.Linq" #>
<#@ import namespace="System.Text" #>
<#@ import namespace="System.Collections.Generic" #>
using weka.attributeSelection;

namespace Ml2.AttrSel.Evals
{
  public class <#= model.TypeName #> : AttributeSelectionEvaluator
  {
    private readonly <#= model.ImplTypeName #> impl = new <#= model.ImplTypeName #>();
    
    <# Array.ForEach(model.Options, o => { #>
    
    public <#= model.TypeName #> <#= o.OptionName #> (<#= o.OptionType #> value) {
      impl.<#= o.OptionImplSetterName #>(value);
      return this;
    }

    <# }) #>
        
    public ASEvaluation GetImpl() { return impl; }
  }
}